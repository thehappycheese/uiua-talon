zero  : "0"
one   : "1"
two   : "2"
three : "3"
four  : "4"
five  : "5"
six   : "6"
seven : "7"
eight : "8"
nine  : "9"

escape:
    key(escape)
space: " "
backspace:
    key(backspace)
junk:
    key(backspace)
delete:
    key(delete)
enter:
    key(enter)
    
sticky line above:
    key(ctrl-k)
    key(ctrl-shift-enter)
reverse and collect lines:
    key(ctrl-k)
    key(ctrl-shift-r)